{

  "manifest_version": 2,
  "name": "Pin'em All",
  "version": "1.0.1",



  "description": "Allows user to pin all open tabs or to unpin all pinned tabs",
  "icons": {
    "128": "images/128.png",
    "48": "images/48.png",
    "16": "images/16.png"
  },



  "browser_action": {
    "default_icon":{
      "128": "images/128.png",
      "48": "images/48.png",
      "16": "images/16.png"
    },

    "default_title": "Pin'em ALl",
    "default_popup": "popup.html"
  }



}
